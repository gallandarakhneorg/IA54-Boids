package fr.utbm.boids

class EnvObjet {
	
	var position : Vector
	
	/* 
	 * GETTER
	 */
	 
	def getPosition() : Vector {
		return this.position
	}
	
	/* 
	 * SETTER
	 */
	 
	def setPosition(p : Vector) : void {
		this.position = p
	}
	
	@SuppressWarnings("equals_with_null")
	def toString() : String {
		if (this.position != null) return "position : " + this.position.toString else return ""
	}
	
}
